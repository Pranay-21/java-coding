import java.util.Scanner;

public class Main{
      public static void main(String[] args){
            int n;
            long fact=1;
            Scanner sc = new Scanner(System.in);
            System.out.print("Enter a number: ");
            n = sc.nextInt();
            System.out.print("The factorial of "+n+" is: ");
            for(int i = 1; i<=n; i++){
                  fact*=i;
            }
            System.out.println(fact);
      }
}
