import java.util.Scanner;

public class Factor{
      public static void main(String[] args){
            int num;
            Scanner sc = new Scanner(System.in);
            System.out.print("Enter the number: ");
            num = sc.nextInt();
            System.out.println("The factors of "+num+" are: ");
            for(int i=1; i<= num; i++){
                  if(num%i == 0){
                        System.out.print(i+" ");
                  }
            }
      }
}
