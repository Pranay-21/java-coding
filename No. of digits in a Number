import java.util.Scanner;

public class Main{
      public static void main(String[] args){
            int count = 0;
            long num;
            Scanner sc = new Scanner(System.in);
            System.out.print("Enter any number: ");
            num = sc.nextInt();
            System.out.print("The number of digits in "+num+" are: ");
            while(num!=0){
                  num/=10;
                  count++;
            }
            System.out.println(count);
      }
}
